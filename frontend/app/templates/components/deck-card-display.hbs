<div class="col-xs-4 col-sm-2 collection-card hover-float">
  {{#if incomplete}}
    <div class="float-container"></div>
  {{/if}}
  <img src="{{card.img}}" class="img-responsive" alt="{{card.name}}">
  {{#if incomplete}}
    <div class="card-count error">{{card.count}}/{{cardDeck.count}}</div>
  {{else}}
    <div class="card-count">{{card.count}}</div>
  {{/if}}
</div>
